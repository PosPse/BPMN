	34 [label="the restoration request is received" shape=polygon]
	35 [label="all possible errors are tracked" shape=polygon]
	36 [label="Based on the tracked errors , all necessary corrective actions are undertaken by Resource Provisioning" shape=polygon]
	37 [label="a trouble - shooting report is created" shape=polygon]
	38 [label="This report is sent out to Service Management" shape=polygon]
	39 [label="the process ends" shape=polygon]
	40 [label="The trouble - shooting report is received by Service Management and this information goes then into the creation of the problem resolution report just as described for ii )" shape=polygon]
