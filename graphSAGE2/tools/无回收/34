	20 [label="a notification about the normal service execution is created" shape=polygon]
	21 [label="this will be analyzed by Resource Provisioning and a trouble report is created" shape=polygon]
	22 [label="Either trouble report or the normal execution notification will be included in a status report and sent back to Service Management" shape=polygon]
	23 [label="Service Management then prepares the final status report based on the received information" shape=polygon]
	24 [label="it has to be determined what counter measures should be taken depending on the information in the final status report" shape=polygon]
	25 [label="Three alternative process paths may be taken" shape=polygon]
	26 [label="the actual service performance is sent back to the Customer Service" shape=polygon]
	27 [label="Service Management will undertake corrective actions by themselves" shape=polygon]
	28 [label="the problem resolution report is created and then sent out to Customer Service" shape=polygon]
